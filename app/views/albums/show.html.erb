<h2><%= @album.name %></h2>
<%= link_to "Edit Album", edit_album_url %>
<%= button_to "Delete Album", album_url(@album), method: :delete %>

<p>Album's songs</p>
<ul>
<% @album.tracks.each do |track| %>
  <li><%= link_to track.name, track_url(track) %></li>
<% end %>
</ul>

<p><%= link_to "Add more songs", new_album_track_url(@album) %></p>

<%= link_to "Back to band", band_url(@album.band) %>
<%= link_to "Back to all bands", bands_url %>
